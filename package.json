{
  "name": "webdriverio_v5_typescript",
  "version": "1.0.0",
  "description": "Boilerplate project using version 5 WebdriverIO & TypeScript.",
  "repository": "https://github.com/jpolley/WebdriverIO_v5_TypeScript",
  "scripts": {
    "test": "node ./node_modules/@wdio/cli/bin/wdio.js wdio.conf.js",
    "test-sauce": "node ./node_modules/@wdio/cli/bin/wdio.js sauce.conf.js",
    "test-cucumber": "node ./node_modules/@wdio/cli/bin/wdio.js cucumber.conf.js",
    "lint": "tslint --fix 'test/**/*.ts' && tslint --fix 'src/**/*.ts'",
    "report": "allure generate ./allure-results --clean && allure open",
    "report-ci": "allure generate allure-results --clean -o allure-report"
  },
  "author": "Jeremy Polley",
  "license": "ISC",
  "devDependencies": {
    "@rpii/wdio-html-reporter": "6.0.4",
    "@types/chai": "^4.2.11",
    "@types/cucumber": "^6.0.1",
    "@types/mocha": "^7.0.2",
    "@types/node": "^13.13.2",
    "@wdio/allure-reporter": "^6.1.1",
    "@wdio/cli": "^6.1.2",
    "@wdio/cucumber-framework": "^6.1.1",
    "@wdio/junit-reporter": "^6.1.2",
    "@wdio/local-runner": "^6.1.2",
    "@wdio/mocha-framework": "^6.1.0",
    "@wdio/sauce-service": "^6.1.0",
    "@wdio/selenium-standalone-service": "^6.0.16",
    "@wdio/spec-reporter": "^6.0.16",
    "@wdio/sync": "^6.1.0",
    "allure-commandline": "^2.13.0",
    "chai": "^4.2.0",
    "chromedriver": "^81.0.0",
    "expect": "^25.4.0",
    "expect-webdriverio": "^1.1.0",
    "log4js": "^6.2.1",
    "mocha": "^7.1.1",
    "ts-node": "^8.9.0",
    "tsconfig-paths": "^3.9.0",
    "tslint": "^6.1.1",
    "typescript": "^3.8.3",
    "wdio-chromedriver-service": "^6.0.2",
    "webdriverio": "^6.1.2"
  }
}
