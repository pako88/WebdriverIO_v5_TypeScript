{
  "name": "webdriverio_v5_typescript",
  "version": "1.0.0",
  "description": "Boilerplate project using version 5 WebdriverIO & TypeScript.",
  "repository": "https://github.com/jpolley/WebdriverIO_v5_TypeScript",
  "scripts": {
    "test": "node ./node_modules/@wdio/cli/bin/wdio.js wdio.conf.js",
    "test-cucumber": "node ./node_modules/@wdio/cli/bin/wdio.js cucumber.conf.js",
    "lint": "tslint --fix 'test/**/*.ts' && tslint --fix 'src/**/*.ts'",
    "report": "allure generate ./allure-results --clean && allure open",
    "report-ci": "allure generate allure-results --clean -o allure-report"
  },
  "author": "Jeremy Polley",
  "license": "ISC",
  "devDependencies": {
    "@rpii/wdio-html-reporter": "0.8.6",
    "@types/chai": "^4.2.11",
    "@types/cucumber": "^6.0.1",
    "@types/mocha": "^7.0.2",
    "@types/node": "^13.9.1",
    "@wdio/allure-reporter": "^5.22.0",
    "@wdio/cli": "^5.22.0",
    "@wdio/cucumber-framework": "^5.20.0",
    "@wdio/junit-reporter": "^5.18.6",
    "@wdio/local-runner": "^5.21.0",
    "@wdio/mocha-framework": "^5.18.7",
    "@wdio/selenium-standalone-service": "^5.16.10",
    "@wdio/spec-reporter": "^5.18.7",
    "@wdio/sync": "^5.20.1",
    "allure-commandline": "^2.13.0",
    "chai": "^4.2.0",
    "expect": "^25.1.0",
    "expect-webdriverio": "^0.2.0",
    "log4js": "^6.1.2",
    "mocha": "^7.1.0",
    "ts-node": "^8.6.2",
    "tsconfig-paths": "^3.9.0",
    "tslint": "^6.1.0",
    "typescript": "^3.8.3",
    "webdriverio": "^5.21.0"
  }
}
